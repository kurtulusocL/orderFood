@model IEnumerable<OrderFood.Entity.Models.Picture>
@{
    Layout = null;
}

@foreach (var item in Model)
{
    <a data-big-image="/img/foto/@item.ImageUrl" class="simpleLens-lens-image" data-lens-image="/img/foto/@item.ImageUrl" href="#">
        <img src="~/img/foto/@item.ImageUrl" style="max-height:300px;max-width:200px;min-height:300px;min-width:200px" alt="@item.ImageUrl" title="@item.Product.Name" class="simpleLens-big-image">
    </a>
} 