@model IEnumerable<OrderFood.Entity.Models.Picture>
@{
    Layout = null;
}

@foreach (var item in Model)
{
    if (item.ProductId != null)
    {
        <a class="aa-product-img" href="~/Product/Detail/@item.ProductId">
            <img src="~/img/foto/@item.ImageUrl" style="max-height:300px;max-width:240px;min-height:300px;min-width:240px" alt="@item.ImageUrl">
        </a>
    }
}