@model IEnumerable<OrderFood.Entity.Models.City>
@{
    Layout = null;
}

@foreach (var item in Model)
{
    <li>
        <a href="~/City/Detail/@item.Id">
            <img src="~/img/cityyy.jpg" title="@item.Name" alt="city" style="max-height:20px;max-width:23px;min-height:20px;min-width:23px;">@item.Name
        </a>
    </li>
}