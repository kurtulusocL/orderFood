
@{
    TempData["Title"] = "Olvidé Mi Contraseña";
    ViewBag.Title = "Se produjo un error al cambiar su contraseña. No se preocupe, intentaremos solucionarlo lo antes posible. No deje de intentarlo de nuevo mientras solucionamos este error. Te deseamos un buen día.";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section id="aa-catg-head-banner">
    <img src="~/Content/temp/web/img/fashion/fashion-header-bg-8.jpg" alt="fashion img">
    <div class="aa-catg-head-banner-area">
        <div class="container">
            <div class="aa-catg-head-banner-content">
                <h2>@TempData["Title"]</h2>
                <ol class="breadcrumb">
                    <li><a href="~/Home/Index">Inicio</a></li>
                    <li class="active">@TempData["Title"]</li>
                </ol>
            </div>
        </div>
    </div>
</section>
<section id="aa-blog-archive">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="aa-blog-archive-area">
                    <div class="row">
                        <div class="col-md-9">
                            <div class="aa-blog-content aa-blog-details">
                                <article class="aa-blog-content-single">
                                    <h2><a>@TempData["Title"]</a></h2>
                                    <div class="aa-article-bottom">
                                        <div class="aa-post-date">
                                            @DateTime.Now.ToLongDateString()
                                        </div>
                                    </div>
                                    <blockquote>
                                        @ViewBag.Title
                                    </blockquote>
                                </article>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>